{
  "name": "@abstract-money/abstract.js",
  "version": "0.17.9",
  "description": "Typings for Abstract smart contracts",
  "homepage": "https://github.com/Abstract-SDK/abstract.js#readme",
  "author": "adairrr",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abstract-SDK/abstract.js.git"
  },
  "bugs": {
    "url": "https://github.com/Abstract-SDK/abstract.js/issues"
  },
  "scripts": {
    "buildold": "tsc",
    "build:cjs": "rimraf ./lib && tsc",
    "build:es": "rimraf ./esm && tsc -m esNext --outDir esm",
    "build": "pnpm build:cjs && pnpm build:es",
    "test": "jest",
    "fix": "./fixes.sh",
    "format": "prettier --write ./src/**/**/*.ts",
    "lint": "eslint 'src/**/*.ts{,x}'",
    "lint:fix": "eslint 'src/**/*.ts{,x}' --quiet --fix",
    "prepublishOnly": "pnpm run build",
    "docs:gen": "typedoc",
    "docs:serve": "serve ./docs -l 3050"
  },
  "dependencies": {
    "@abstract-money/cosmwasm": "workspace:*",
    "@blazed-space/blz-typedoc-theme": "^0.0.2",
    "@cosmjs/amino": "^0.31.0",
    "@cosmjs/cosmwasm-stargate": "^0.31.0",
    "@cosmjs/encoding": "^0.31.0",
    "@cosmjs/tendermint-rpc": "^0.31.0",
    "cosmjs-types": "^0.8.0",
    "rimraf": "^3.0.2",
    "semver": "^7.5.2",
    "ts-mixer": "^6.0.2",
    "ts-pattern": "^4.0.6",
    "type-fest": "^3.5.0"
  },
  "peerDependencies": {
    "@cosmjs/amino": ">=0.28",
    "@cosmjs/cosmwasm-stargate": ">=0.28",
    "@cosmjs/encoding": "^>=0.28"
  },
  "devDependencies": {
    "@cosmjs/amino": "^0.31.0",
    "@cosmjs/cosmwasm-stargate": "^0.31.0",
    "@cosmjs/encoding": "^0.31.0",
    "@cosmjs/stargate": "^0.31.0",
    "@types/jest": "^29.5.2",
    "@types/ledgerhq__hw-transport": "^4.21.4",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "jest": "^29.6.0",
    "prettier": "^2.7.0",
    "serve": "^14.2.0",
    "ts-jest": "^29.0.2",
    "tslib": "^2.3.1",
    "typedoc": "^0.24.8",
    "typedoc-theme-hierarchy": "^4.0.0",
    "typescript": "4.9.3"
  }
}

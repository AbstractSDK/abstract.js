{
  "name": "@abstract-money/abstract.js-react",
  "version": "0.17.9",
  "description": "Typings for Abstract smart contracts with react query",
  "homepage": "https://github.com/Abstract-SDK/abstract.js#readme",
  "author": "adairrr",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abstract-SDK/abstract.js.git"
  },
  "bugs": {
    "url": "https://github.com/Abstract-SDK/abstract.js/issues"
  },
  "scripts": {
    "buildold": "tsc",
    "build:cjs": "rimraf ./lib && tsc",
    "build:esm": "rimraf ./esm && tsc -m esNext --outDir esm",
    "build": "pnpm build:cjs && pnpm build:esm",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fix": "./fixes.sh",
    "format": "prettier --write ./src/**/**/*.ts",
    "lint": "eslint 'src/**/*.ts{,x}'",
    "lint:fix": "eslint 'src/**/*.ts{,x}' --quiet --fix",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "type-fest": "^3.5.0",
    "@abstract-money/cw-query-hooks": "workspace:*",
    "@abstract-money/abstract.js": "workspace:*"
  },
  "peerDependencies": {
    "@cosmjs/amino": ">=0.28",
    "@cosmjs/cosmwasm-stargate": ">=0.28",
    "@tanstack/react-query": ">=4",
    "react": ">=18"
  },
  "devDependencies": {
    "@cosmjs/amino": "^0.31.0",
    "@cosmjs/cosmwasm-stargate": "^0.31.0",
    "@cosmjs/encoding": "^0.31.0",
    "@tanstack/react-query": "^4.0.5",
    "@types/ledgerhq__hw-transport": "^4.21.4",
    "@types/node": "^18.11.10",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "cosmjs-types": "^0.8.0",
    "eslint": "^8.17.0",
    "prettier": "^2.7.0",
    "ts-pattern": "^4.0.6",
    "typescript": "4.9.3",
    "tslib": "^2.3.1"
  }
}

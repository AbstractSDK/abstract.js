{
  "name": "@abstract-money/core",
  "version": "0.0.0",
  "description": "Typings for Abstract smart contracts",
  "homepage": "https://github.com/AbstractSDK/abstract.js#readme",
  "author": "adairrr <adair@abstract.money>",
  "contributors": [
    "h0tw4t3r <vlad@abstract.money>",
    "adairrr <adair@abstract.money>"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AbstractSDK/abstract.js.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/AbstractSDK/abstract.js/issues"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "docs:gen": "typedoc",
    "docs:serve": "serve ./docs -l 3050",
    "typecheck": "tsc --noEmit --jsx react-jsx",
    "codegen:abstract": "abstract generate",
    "codegen:subgraph": "graphql-codegen --config codegen.yml",
    "codegen:subgraph:watch": "graphql-codegen --watch"
  },
  "dependencies": {
    "@abstract-money/cosmwasm-utils": "workspace:*",
    "@chain-registry/types": "^0.13.1",
    "@cosmjs/amino": "^0.31.0",
    "@cosmjs/cosmwasm-stargate": "^0.31.0",
    "@cosmjs/encoding": "^0.31.0",
    "@cosmjs/stargate": "^0.31.0",
    "@cosmjs/tendermint-rpc": "^0.31.0",
    "@rimbu/bimap": "^1.1.1",
    "bech32": "^2.0.0",
    "chain-registry": "^1.18.1",
    "cosmjs-types": "^0.8.0",
    "graphql-request": "5",
    "lodash": "^4.17.21",
    "semver": "^7.3.8",
    "ts-mixer": "^6.0.2",
    "ts-pattern": "^4.0.6",
    "type-fest": "^3.5.0",
    "yup": "1.3.2"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "type": "module",
  "devDependencies": {
    "@abstract-money/cli": "workspace:*",
    "@graphql-codegen/cli": "^2.13.1",
    "@graphql-codegen/client-preset": "^1.2.6",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/jest": "^29.5.2",
    "@types/ledgerhq__hw-transport": "^4.21.4",
    "@types/lodash": "^4.14.201",
    "@types/node": "^18.11.10",
    "@types/semver": "^7.3.8",
    "jest": "^29.6.0",
    "prettier": "^2.7.0",
    "rimraf": "^3.0.0",
    "serve": "^14.2.0",
    "ts-jest": "^29.0.2",
    "typedoc": "^0.23.25",
    "typedoc-plugin-markdown": "^3.14.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "default": "./dist/api/index.js"
    },
    "./chain-registry": {
      "types": "./dist/chain-registry/index.d.ts",
      "default": "./dist/chain-registry/index.js"
    },
    "./clients": {
      "types": "./dist/clients/index.d.ts",
      "default": "./dist/clients/index.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "default": "./dist/tools/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "/api",
    "/chain-registry",
    "/clients",
    "/tools",
    "/utils",
    "/dist"
  ],
  "sideEffects": false
}

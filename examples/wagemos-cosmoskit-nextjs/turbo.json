{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "pipeline": {
    "build": {
      "dependsOn": [
        "@abstract-money/provider-cosmoskit#build",
        "@abstract-money/react#build",
        "generate"
      ],
      "outputs": [".next/**"]
    },
    "dev": {
      "dependsOn": [
        "@abstract-money/provider-cosmoskit#build",
        "@abstract-money/react#build",
        "generate"
      ],
      "cache": false
    },
    "typecheck": {
      "dependsOn": [
        "@abstract-money/provider-cosmoskit#build",
        "@abstract-money/react#build",
        "generate"
      ]
    },
    "generate": {
      "outputs": ["node_modules/graz/chains"]
    }
  }
}
